//find missing no
class HelloWorld {
    public static void missingEle(int[]arr)
    {
        int n=arr.length+1;
        int total_sum=n*(n+1)/2;
        int curr_sum=0;
        for(int i=0;i<arr.length;i++)
        {
            curr_sum=curr_sum+arr[i];
        }
        System.out.println("missing no"+(total_sum-curr_sum));
    }
    public static void main(String[] args) {
    int []arr={1,2,3,4,6};
    missingEle(arr);
    }
}
