// reverse String
class HelloWorld {
    public static void main(String[] args) {
        String  str="priya";
        char []arr=str.toCharArray();
        int low=0,high=arr.length-1;
        while(low<high)
        {
        char temp=arr[low];
        arr[low]=arr[high];
        arr[high]=temp;
        low++;
        high--;
        }
        for(int i=0;i<arr.length;i++)
        {
            System.out.println(arr[i]);
        }
    }
}
