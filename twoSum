//brute force solution:
class HelloWorld {
    public static void main(String[] args) {
        int arr[]={8,3,4,5,1};
        int target=6;
        for(int i=0;i<arr.length-1;i++)
        {
           for(int j=i+1;j<arr.length;j++)
           {
               if(arr[i]+arr[j]==target)
               {
                   System.out.println(arr[i]+" "+arr[j]);
               }
           }
        }
     }
    }
